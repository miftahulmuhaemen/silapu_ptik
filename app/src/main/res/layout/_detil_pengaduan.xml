<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/medium_margin_globally">


    <studio.carbonylgroup.textfieldboxes.TextFieldBoxes
        android:id="@+id/ti_aduan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:useDenseSpacing="true"
        app:labelText="Aduan"
        app:enabled="false"
        >
        <studio.carbonylgroup.textfieldboxes.ExtendedEditText
            android:id="@+id/edt_aduan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="@string/dialog_register"
            android:textColor="@color/Black"
            />
    </studio.carbonylgroup.textfieldboxes.TextFieldBoxes>



    <studio.carbonylgroup.textfieldboxes.TextFieldBoxes
        android:id="@+id/ti_perihal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/medium_margin_globally"
        app:useDenseSpacing="true"
        app:labelText="Perihal"
        app:enabled="false"
        >

        <studio.carbonylgroup.textfieldboxes.ExtendedEditText
            android:id="@+id/edt_perihal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="@string/dialog_register"
            android:textColor="@color/Black"
            />

    </studio.carbonylgroup.textfieldboxes.TextFieldBoxes>

</LinearLayout>